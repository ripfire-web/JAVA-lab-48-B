class overidingFUNC{
 
   void show()
   {
	System.out.println("This is running overriding function");
   }	   
}
class Demo extends overidingFUNC{
@override

 
   void show()
   {
	System.out.println("This is overriding method of parent");
   }
}
   class Main{
   
   public static void main( String args[])
   {
	overidingFUNC obj = new overidingFUNC();
	obj.disp();

	overidingFUNC obj2 = new Demo();
	obj2.disp();
   }
}
